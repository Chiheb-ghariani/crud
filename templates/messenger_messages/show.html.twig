{% extends 'base.html.twig' %}

{% block title %}MessengerMessages{% endblock %}

{% block body %}
    <h1>MessengerMessages</h1>

    <table class="table">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ messenger_message.id }}</td>
            </tr>
            <tr>
                <th>Body</th>
                <td>{{ messenger_message.body }}</td>
            </tr>
            <tr>
                <th>Headers</th>
                <td>{{ messenger_message.headers }}</td>
            </tr>
            <tr>
                <th>QueueName</th>
                <td>{{ messenger_message.queueName }}</td>
            </tr>
            <tr>
                <th>CreatedAt</th>
                <td>{{ messenger_message.createdAt ? messenger_message.createdAt|date('Y-m-d H:i:s') : '' }}</td>
            </tr>
            <tr>
                <th>AvailableAt</th>
                <td>{{ messenger_message.availableAt ? messenger_message.availableAt|date('Y-m-d H:i:s') : '' }}</td>
            </tr>
            <tr>
                <th>DeliveredAt</th>
                <td>{{ messenger_message.deliveredAt ? messenger_message.deliveredAt|date('Y-m-d H:i:s') : '' }}</td>
            </tr>
        </tbody>
    </table>

    <a href="{{ path('app_messenger_messages_index') }}">back to list</a>

    <a href="{{ path('app_messenger_messages_edit', {'id': messenger_message.id}) }}">edit</a>

    {{ include('messenger_messages/_delete_form.html.twig') }}
{% endblock %}
